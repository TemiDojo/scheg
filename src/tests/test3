;;;; ====================
;;;; Pairs (cons, car, cdr)
;;;; ====================
;(cons 1 2)
;(cons )
;(cons 3)
;(cons ())
;(cons () 4)
;(cons 1 ())
;(cons (cons (cons (cons ()))))
;(cons 1 (cons 2 (cons 3 (cons 4 (cons 5 ())))))

;;; car
;(car )
;(car 2)
;(car (cons 1 3))
;(car (cons (cons 1 2)))
;(car (car (cons 1 (car (cons () 3))))) ; should check type passed to car

;;; cdr
;(cdr )
;(cdr 2)
;(cdr (cons 1 3))
;(cdr (cons (cons 1 2)))
;(cdr (cdr (cons 1 (cdr (cons () 3))))) ; should check type passed to cdr

;;;; ====================
;;;; String
